<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">#FFFFFF</item>
        <item name="colorAccent">@color/bg_skyblue</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="titleTextColor">@color/black_hex</item>
        <item name="elevation">10dp</item>
        <item name="android:homeAsUpIndicator">@drawable/back</item>
        <item name="android:shadowColor">@color/grey</item>
    </style>

    <style name="RippleStyleWhite">
        <item name="mrl_rippleOverlay">true</item>
        <item name="mrl_rippleColor">#80FFFFFF</item>
        <item name="mrl_rippleHover">true</item>
        <item name="mrl_rippleAlpha">0.2</item>
    </style>
</resources>
